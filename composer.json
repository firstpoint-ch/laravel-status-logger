{
    "name": "firstpoint-ch/laravel-status-logger",
    "description": "Status logs for Laravel models",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Marmillod",
            "email": "fmarmillod@firstpoint.ch"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "StatusLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StatusLogger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StatusLogger\\StatusLoggerServiceProvider"
            ]
        }
    }
}
